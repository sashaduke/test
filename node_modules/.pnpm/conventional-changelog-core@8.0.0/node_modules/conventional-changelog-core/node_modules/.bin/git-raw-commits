#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/shulgin/repos/test/node_modules/.pnpm/git-raw-commits@5.0.0_conventional-commits-parser@6.0.0/node_modules/git-raw-commits/src/node_modules:/home/shulgin/repos/test/node_modules/.pnpm/git-raw-commits@5.0.0_conventional-commits-parser@6.0.0/node_modules/git-raw-commits/node_modules:/home/shulgin/repos/test/node_modules/.pnpm/git-raw-commits@5.0.0_conventional-commits-parser@6.0.0/node_modules:/home/shulgin/repos/test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/shulgin/repos/test/node_modules/.pnpm/git-raw-commits@5.0.0_conventional-commits-parser@6.0.0/node_modules/git-raw-commits/src/node_modules:/home/shulgin/repos/test/node_modules/.pnpm/git-raw-commits@5.0.0_conventional-commits-parser@6.0.0/node_modules/git-raw-commits/node_modules:/home/shulgin/repos/test/node_modules/.pnpm/git-raw-commits@5.0.0_conventional-commits-parser@6.0.0/node_modules:/home/shulgin/repos/test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../git-raw-commits@5.0.0_conventional-commits-parser@6.0.0/node_modules/git-raw-commits/src/cli.js" "$@"
else
  exec node  "$basedir/../../../../../git-raw-commits@5.0.0_conventional-commits-parser@6.0.0/node_modules/git-raw-commits/src/cli.js" "$@"
fi
